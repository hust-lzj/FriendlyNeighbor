.header{
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  box-sizing: border-box;
  z-index: 1;
  position: absolute;
  left: 10vw;
  right: 10vw;
  top: 4vh;
}
.container{
  flex: 1;
  position: absolute;
  display: flex;
  flex-direction: column;
  height: 35vh;
  background-color: #ffffff;
  width:100vw;
  border-bottom-left-radius: 10%;
  border-bottom-right-radius: 10%;

}
.container1{
  display: flex;
  width: 100%;
  height: 100%;
  flex-direction: column;
}
.container2{
  display: flex;
  margin-top: 4vh;
  margin-bottom: 5px;
  flex-direction: column;
}
.container2-input{
  display: flex;
  margin-top: 2vh;
  margin-bottom: 2vh;
  flex-direction: column;
  background-color: #ffffff;
  width: 85vw;
  height: 5vh;
  border-radius:25px;
  padding-left: 1vw;
}
.container3{
  position: absolute;
  top: 92.5vh;
  display: flex;
  left: 6vw;
  width:88vw;
  flex-direction: row;
}
.bottom-picture{
  position: absolute;
  top: 92vh;
  left: 5vw;
  z-index: 0;
  width: 90vw;
}
.back-picture{
  width: 30px;
  height: 30px;
  position: absolute;
  z-index: 2;
  top:3.5vh;
}
.container4{
  position: absolute;
  top: 35vh;
  display: flex;
  margin-left: 7vw;
  flex-direction: column;
}
.top-picture{
  position: absolute;
  width:100vw;
  top: 0vh;
  z-index: 0;
}
.bt-box text{
  width: 30%;
  height: 80%;
  color: black;
  background-color: #ffffff;
  border-width:thin;
  text-align: center;
  border: solid rgb(207, 203, 203);
}
.bt-box{
  display: flex;
  width: 80vw;
  height: 4vh;
  margin-left: 4vw;
  margin-right: 15vw;
  z-index: 4;
  justify-content: space-evenly;
}
.page-container{
  background-image: linear-gradient(180deg,rgba(15,219,134,1),rgba(14,208,208,1));
  height: 100vh;
}

