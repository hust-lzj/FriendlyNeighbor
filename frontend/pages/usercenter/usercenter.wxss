.page-container{
  background-image: linear-gradient(180deg,rgba(15,219,134,1),rgba(14,208,208,1));
  height: 100vh;
}
.container{
  flex: 1;
  display: flex;
  flex-direction: row;
  height: 100px;
  width: 375px;
}
.container1{
  display: flex;
  flex-direction: column;
  margin-top: 60px;
  margin-bottom: 0px;
  z-index: 1;
}
.container .picture1{
  width: 50px;
  height: 50px;
  border-radius: 25px;
  margin-right: 20px;
  margin-left: 20px;
  margin-top: 55px;
  /* border: solid red; */
  z-index: 1;
}
.picture2{
  position: absolute;
  z-index: 0;
  width: 375px;
  height: 667px;
}
.button_container{
  position: absolute;
  display: flex;
  flex-direction: column;
  left: 15px;
  top: 120px;
}
.button_set{
  background-color:transparent;
  position: flex;
  flex-direction: row;
  margin-top: 30px;
}
.btnImg{
  width: 30px;
  height: 30px;
}
.btntext{
  position: relative;
  bottom: 10px;
  margin-left: 15px;
  color: #ffffff;
  font-size: large;
}

.back-picture{
  width: 30px;
  height: 30px;
  position: absolute;
  z-index: 2;
  top:20px;
}
.button_set1{
